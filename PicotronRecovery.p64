--[[pod_format="raw",created="2024-06-21 04:33:41",modified="2024-07-05 02:26:06",revision=179]]
state = 0
confirm = false

function _init()

end

function _update()
	cls()
	if confirm then
		print("Are you sure?")
		print("Y/N:_")
		if keyp("y") then
			confirm = false
			awaitkey = true
		elseif keyp("n") then
			confirm = false
			state = 0
			awaitkey = true
		end	
	elseif state == 0 then
		print("Picotron Recovery Tool (v0.0.1a/online/nogui): Select Option")
		print("Developed by Ben3Coder Software.")
		print("")
		print("	[1] : Reset Picotron Operating System")
		print("			All user data on this partition will be reset,")
		print("			the entire Picotron System will be reset.")
		print("")
		print("	[2] : Reinstall System Coretools")
		print("			This will reinstall all applications that came installed")
		print("			with the system and reboot after.")
		print("")
		print("	[3] : Reboot into CLI-only mode")
		print("			The system will reboot in CLI-only mode with full access")
		print("			to modify anything inside the partition.")
		print("")
		print("	[4] : Do nothing and exit")
		print("")
		print("Please select your option: _")
		print("")
		print("Downloading and extracting recovery tools...")
		print("Mounted /recovery")
		if keyp("1") then
			state = 1
			confirm = true
		elseif keyp("2") then
			state = 2
			confirm = true
		elseif keyp("3") then
			exit(0)
			confirm = false
		end
	elseif state == 1 then
		tind = 0
		while (fstat("/localsystmp"..tind..".p64") and tind < 128) tind += 1
		cp("/system", "/localsystmp"..tind..".p64")
		rm("/system")
		cp("/localsystmp"..tind..".p64", "/system")
		rm("/localsystmp"..tind..".p64")
		state = 0
	elseif state == 2 then
		rm("/system")
		send_message(2, {event="reboot"})
	end
end
:: [eoc]
